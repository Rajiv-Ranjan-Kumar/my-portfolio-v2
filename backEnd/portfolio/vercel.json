{
    "version": 2,
    "builds": [
        {
            "src": "config/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.12"
            }
        },
        {
            "src": "build.sh",
            "use": "@vercel/static_build",
            "config": {
                "distDir": "staticfiles_build"
            }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "des": "/static/$1"
        },
        {
            "src": "/(.*)",
            "des": "congig/wsgi.py"
        }
    ]
}